<section>
  <h2>Crie sua própria sala</h2>
  <form action="/create-room" method="POST">
    <label class="sr-only" for="room-pass">Código da sala</label>
    <input
      name="password"
      type="password"
      id="room-pass"
      placeholder="Insira uma senha"
    />
    <button>
      <img src="images/users-white.svg" alt="Criar sala" />
      Criar sala
    </button>
  </form>
</section>
